{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "Proxy": {
      "Url": [
        "http://localhost:5689/sqlhandler/",
        "https://localhost:5690/sqlhandler/"
      ],
        "Servers": [
            {
                "Name": "Northwind",
                "Provider": "SQLServer",
                "ConnectionString": "Data Source=localhost\\SQLEXPRESS;initial catalog=Northwind;Integrated Security=SSPI;Packet Size=4096;"
            },
          {
            "Name": "SQLiteNorthwind",
            "Provider": "SQLite",
            "ConnectionString": "Provider=SQLite;Data Source=..\\Northwind\\db\\Northwind.db;Version=3; DateTimeFormat=Ticks; Pooling=True; Max Pool Size=100;"
          }
        ]
    }
}
