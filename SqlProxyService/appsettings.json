{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "Proxy": {
        "Url": [
            "http://localhost/sqlhandler/"
        ],
        "Servers": [
            {
                "Name": "Northwind",
                "Provider": "SQLServer",
                "ConnectionString": "data source=localhost\\sqlexpress;initial catalog=Northwind;integrated security=SSPI;packet size=4096;"
            },
            {
                "Name": "LocalNorthwind",
                "Provider": "SQLite",
                "ConnectionString": "c:\\db\\db\\Northwind.db"
            }
        ]
    }
}
